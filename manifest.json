{

  "manifest_version": 2,
  "name": "AutoCanvas",
  "version": "1.0",

  "description": "Skips NJIT's COVID page to get into canvas",

  "icons": {
    "48": "icons/auto-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://webauth.njit.edu/idp/profile/SAML2/Redirect/SSO*"],
      "js": ["autocanvas.js"]
    }
  ]

}